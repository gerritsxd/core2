RewriteCond %{HTTP_HOST} ^stab\.opens\.science$ [OR]
RewriteCond %{HTTP_HOST} ^www\.stab\.opens\.science$
RewriteRule ^website/drive-folder(.*)$ "https://drive.google.com/drive/folders/1MCol8WK1xMKaynkWS-iXlSgomE5FP4bX?usp=drive_link" [R=302,L]

RewriteCond %{HTTP_HOST} ^stab\.opens\.science$ [OR]
