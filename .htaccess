
RewriteEngine on

### Directory used to be called CoRe; but now when Plesk clones or
### pulls the git repo, it creates a directory called CoRe anyway,
### even though it was renamed...
RewriteCond %{HTTP_HOST} ^stab\.opens\.science$ [OR]
RewriteCond %{HTTP_HOST} ^www\.stab\.opens\.science$
RewriteRule ^CoRe/(.*)$ /core/$1 [R=301]


RewriteCond %{HTTP_HOST} ^stab\.opens\.science$ [OR]
RewriteCond %{HTTP_HOST} ^www\.stab\.opens\.science$
